{
  ".workspace .editor[data-grammar~='glsl']:not(.mini)": {
    "ctrl-enter": "veda:load-shader",
    "ctrl-shift-enter": "veda:watch-shader",
    "ctrl-alt-enter": "veda:watch-active-shader",
    "ctrl-.": "veda:stop-watching",
    "ctrl-cmd-enter": "veda:load-sound-shader",
    "ctrl-cmd-.": "veda:stop-sound-shader"
  }
}
